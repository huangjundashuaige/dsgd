    "# mpi_helloworld.py\n",
    "\n",
    "from mpi4py import MPI\n",
    "\n",
    "\n",
    "comm = MPI.COMM_WORLD\n",
    "size = comm.Get_size()\n",
    "rank = comm.Get_rank()\n",
    "node_name = MPI.Get_processor_name() # get the name of the node\n",
    "\n",
    "print('Hello world from process %d at %s.' % (rank, node_name))"
